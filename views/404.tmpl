                    <h1 id="page-title">Page not found</h1>
                    <p class="subtitle">The page you were looking for doesn&#39;t exist or may have been moved.</p>

                    <div class="notfound-code" aria-hidden="true">404</div>

                    <div class="notfound-actions">
                        <a class="btn primary" href="/dashboard">Back to dashboard</a>
                        <a class="btn ghost" href="/domains">View domains</a>
                    </div>
                </div>
            </section>
        </main>
    </div>
</body>

</html>
{{ end }}
